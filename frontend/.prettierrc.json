{
    "tabWidth": 3,
    "printWidth": 100,
    "singleQuote": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "useTabs": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "endOfLine": "lf",
    "embeddedLanguageFormatting": "auto"
}
